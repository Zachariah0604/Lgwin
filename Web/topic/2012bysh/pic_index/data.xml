<?xml version="1.0" encoding="UTF-8"?>
<data>
	<settings>
		<set id="carouselCylinder">
			<radius>300</radius>
			<height>200</height>
			<numImagesPerRow>16</numImagesPerRow>
			<numImagesPerColumn>3</numImagesPerColumn>
			<cameraZoom>90</cameraZoom>
			<cameraPositionZ>300</cameraPositionZ>
			<doubleSided>false</doubleSided>
			<thumbWidth>600</thumbWidth>
			<thumbHeight>600</thumbHeight>
		</set>
		<set id="mainBack">
			<bgcolor>0x000000</bgcolor>
			<height>800</height>
			<width>1000</width>
			<x>0</x>
			<y>0</y>
			<filters>
			</filters>
		</set>
		<set id="titleBar">
			<filters>
				<filter type="DropShadow" color="0xffffff" alpha=".5" blurX="8" blurY="8" distance="0" strength=".65" inner="false" knockout="false" quality="1"></filter>
			</filters>
		</set>
		<set id="titleBarTextFormat">
			<textcolor>0xffffaa</textcolor>
			<size>16</size>
			<font>_typewriter</font>
			<align>center</align>
			<letterSpacing>0</letterSpacing>
			<bold>false</bold>
			<italic>false</italic>
			<kerning>true</kerning>
			<underline>false</underline>
			<filters>
			</filters>
		</set>
		<set id="previewImage">
			<filters>
				<filter type="Bevel" highlightColor="0xffff33" shadowColor="0x33ffff" highlightAlpha="1" shadowAlpha="1" blurX="10" blurY="10" strength="1" knockout="false" quality="1" bevelType="inner" distance="10" angle="45"></filter>
			</filters>
		</set>
		<set id="thumbnailFrame">
			<color>0xffffff</color>
			<thickness>6</thickness>
		</set>
		<set id="previewFrame">
			<color>0xffffff</color>
			<randomColor>false</randomColor>
			<filterGap>20</filterGap>
			<leftMargin>8</leftMargin>
			<rightMargin>8</rightMargin>
			<topMargin>8</topMargin>
			<bottomMargin>8</bottomMargin>
			<filters>
			</filters>
		</set>
		<set id="closeButton">
			<isAvaible>false</isAvaible>
			<position>custom</position>
			<x>640</x>
			<y>66</y>
			<width>30</width>
			<height>30</height>
		</set>
		<set id="previewFrame2">
			<bgcolor>0x222222</bgcolor>
			<width>400</width>
			<height>400</height>
			<filters>
			</filters>
		</set>
	</settings>
	<transitions>
		<transition id="transitionToThumbnails">
			<type>Blinds</type>
			<duration>.5</duration>
			<numStrips>50</numStrips>
			<dimension>1</dimension>
		</transition>
		<transition id="transitionToLargePreview">
			<type>PixelDissolve</type>
			<duration>.5</duration>
			<xSections>50</xSections>
			<ySections>50</ySections>
		</transition>
		<transition id="transitionToCarousel">
			<type>PixelDissolve</type>
			<duration>.5</duration>
			<xSections>50</xSections>
			<ySections>50</ySections>
		</transition>
	</transitions>
