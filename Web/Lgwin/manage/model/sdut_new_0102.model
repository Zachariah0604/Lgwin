</div>
<script type="text/javascript">
    var intNewsSpeed = 9000;
    var intNewsTimerId;
    var intNewsId = 0;
    var strNews_Date = new Array;
    var strNews_Content = new Array;
    var strNews_Link = new Array;
    var dateformat;

    var intControlsindexNewsSysWordingCheckId;
    intControlsindexNewsSysWordingCheckId = setInterval("controlsindexNewsSysWordingCheck()", intSysWordingCheckSpeed);

    function controlsindexNewsSysWordingCheck() {
        if (sysWordingRead) {
            $("#news_area span:first").html($("NewsTitle", sysWording).text() + " ");
            clearTimeout(intControlsindexNewsSysWordingCheckId);
        }
    }

    $(document).ready(function () {
        $("#news_area div").each(function (i) {
            strNews_Content[i] = $("#spanTitle", this).text();
            strNews_Date[i] = date_format($("#spanDate", this).text(), $("DateFormat", sysWording).text());
            strNews_Link[i] = $("#spanLink", this).text();
        });

        get_news();
        intNewsId = intNewsId + 1;
        intNewsTimerId = setInterval("news_timer()", intNewsSpeed);
    });

    function NewsPreLoadSetting(arrData) {
        var val = arrData.split('*');
        dateformat = val[1];
        $("#news_area span:first").html(val[0] + " ");
    }

    function news_timer() {
        reset_news();
        $("#news").fadeOut("slow", function () {
            get_news();
            $("#news").fadeIn("slow", function () {
                intNewsId = intNewsId + 1;
            });
        });
    }

    function reset_news() {
        if (intNewsId == strNews_Date.length) {
            intNewsId = 0;
        }
    }

    function get_news() {
        $("#new_date").html('');
        //$("#newtitle").html(strNews_Content[intNewsId]);
        $("#spanTransTitle").html(strNews_Content[intNewsId]);
        $("#newtitle").html($("#spanTransTitle").text());
        $("#newtitle").attr({ href: strNews_Link[intNewsId] });
    }

    function Next_News() {
        clearTimeout(intNewsTimerId);
        if (intNewsId > strNews_Date.length - 1) {
            intNewsId = 0;
        }
        news_timer();
        intNewsTimerId = setInterval("news_timer()", intNewsSpeed);
    }

    function Previous_News() {
        clearTimeout(intNewsTimerId);
        intNewsId = intNewsId - 2;
        if (intNewsId < 0) {
            intNewsId = strNews_Date.length - 1;
        }
        news_timer();
        intNewsTimerId = setInterval("news_timer()", intNewsSpeed);
    }
</script>
